    "def linestyle_generator():\n",
    "    linestyle = [\"-\",\"--\",\"-.\",\":\"]\n",
    "    lineID = 0\n",
    "    while True:\n",
    "        print(lineID)\n",
    "        yield linestyle[lineID]\n",
    "        lineID = (lineID + 1) % len(linestyle)\n",
    "LS = linestyle_generator()\n",
    "for i in range(5):\n",
    "    print(next(LS))"
   ]
